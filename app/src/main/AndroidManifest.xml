<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vm.kidslearning.kids.kids123">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.User_Manual_Activity"
            android:parentActivityName=".activity.Home_Activity"></activity>
        <activity
            android:name=".activity.TQuestion_Activity"
            android:parentActivityName=".activity.TTestNumber_Activity" />
        <activity
            android:name=".activity.TTestNumber_Activity"
            android:parentActivityName=".activity.TCategory_Activity" />
        <activity
            android:name=".activity.Home_Activity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.TCategory_Activity" />
        <activity
            android:name=".activity.MainActivity"
            android:parentActivityName=".activity.Home_Activity" />
        <activity
            android:name=".activity.Detail_Activity"
            android:screenOrientation="portrait" />
    </application>

</manifest>